            name="root"
            path="." />

        <files-path
            name="files"
            path="." />

        <cache-path
            name="cache"
            path="." />

        <!-- 物理路径为Context.getFilesDir() + /files/* -->
        <files-path
            name="files"
            path="files" />
        <!-- 物理路径为Context.getCacheDir() + /files/* -->
        <cache-path
            name="cache"
            path="files" />
