<?xml version="1.0" encoding="utf-8"?>


<resources>

    <paths>
        <!-- 代表设备的根目录 new File("/") -->
        <root-path
            name="root"
            path="." />
        <!-- context.getFilesDir() -->
        <files-path
            name="files"
            path="." />
        <!-- context.getCacheDir() -->
        <cache-path
            name="cache"
            path="." />
        <!-- context.getExternalFilesDirs() -->
        <external-files-path
            name="name"
            path="path" />
        <!-- context.getExternalCacheDirs() -->
        <external-cache-path
            name="name"
            path="path" />
        <!-- Environment.getExternalStorageDirectory() -->
        <external-path
            name="external"
            path="." />
    </paths>

    <paths>

        <external-path
            name="camera_photos"
            path="" />

        <paths>
            <external-path
                name="files_root"
                path="Android/data/com.alpha.alphaapp/" />
            <external-path
                name="external_storage_root"
                path="." />
        </paths>

        <paths>
            <external-path
                name="my_images"
                path="Pictures" />
            <external-files-path
                name="images"
                path="Pictures" />
            <root-path
                name="root_path"
                path="." />
