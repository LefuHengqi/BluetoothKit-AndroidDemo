        <external-path
            name="external"
            path="files" />
        <!-- 物理路径为Context.getExternalFilesDir(String) + /files/* -->
        <external-files-path
            name="externalfiles"
            path="files" />
        <!-- 物理路径为Context.getExternalCacheDir() + /files/* -->
        <external-cache-path
            name="externalcache"
            path="files" />
        <!-- 物理路径为`Context.getExternalMediaDirs() + /files/*, 要求API21+ -->
        <external-media-path
            name="externalmedia"
            path="files" />
    </paths>

    <paths>

